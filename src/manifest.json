{
  "manifest_version": 2,
  "name": "To Many Tabs",
  "short_name":"To Many Tabs",
  "description": "This extension will notify you when you have more then n tabs open",
  "version": "1.0.0",
  "author": "Fabian Mor√≥n Zirfas",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "notifications"
  ]
}