{
  "manifest_version": 2,
  "name": "To Many Tabs Plugin",
  "description": "This extension will notify you when you have more then n tabs open",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_page": "options.html",

  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "notifications"
  ]
}